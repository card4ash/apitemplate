@{
    ViewData["Title"] = "Users";
    Layout = "~/Views/Shared/_LayoutChameleonAdmin.cshtml";
}



<section id="icon-tabs">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-header">
                    <div class="row" id="menuInfo">
                        <div class="col-6"><h4 class="card-title">Users</h4></div>
                    </div>

                </div>
                <div class="card-content collapse show">
                    <div class="card-body">
                        <table id="userTable" class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>Action</th>
                                    <th>Name</th>
                                    <th>Username</th>
                                    <th> Email </th>
                                    <th> Phone </th>
                                    <th> Website </th>
                                    <th>Company Name</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<div class="modal fade" id="modalMenu" role="dialog" aria-labelledby="exampleModalLabel" style="z-index:1051;">
    <div class="modal-dialog modal-xl" role="document" style="margin-left:10%;">
        <div class="modal-content">
            <div class="modal-header bg-info white">
                <h5 class="modal-title white">Menu</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button id="btnSaveMenu" type="button" class="btn btn-primary">Save changes</button>
            </div>
        </div>
    </div>
</div>



<div class="card">

</div>



@section ScriptSection
{
    <script src="~/js/app/home.js"></script>
}