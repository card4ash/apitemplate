    <title>Digital Lending</title>
    <link rel="shortcut icon" href="~/LayoutAdmin//img/favicon.ico">
    <!-- BEGIN: Vendor CSS-->
    <link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/app-assets/vendors/css/vendors.min.css">
    <link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/app-assets/vendors/css/extensions/toastr.css">    
    <!-- END: Vendor CSS-->
    <!-- BEGIN: Theme CSS-->
    <link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/app-assets/css/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/app-assets/css/bootstrap-extended.css">
    <link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/app-assets/css/colors.css">
    <link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/app-assets/css/components.css">
    <!-- END: Theme CSS-->
    <!-- BEGIN: Page CSS-->
    @*<link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/app-assets/css/core/menu/menu-types/vertical-menu-modern.css">*@
    <link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/app-assets/css/core/colors/palette-gradient.css">
    <link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/app-assets/css/plugins/extensions/toastr.css">
    <link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/app-assets/vendors/css/timeline/vertical-timeline.css">
    <link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/app-assets/fonts/feather/style.css" />
    <!-- END: Page CSS-->

    <!-- BEGIN: Custom CSS-->
    <link rel="stylesheet" type="text/css" href="~/ChameleonAdmin/assets/css/style.css">
    <!-- END: Custom CSS-->

    <link href="~/LayoutAdmin/css/vendors.css" rel="stylesheet" />
    <!-- app style -->
    <link href="~/LayoutAdmin/css/style.css" rel="stylesheet" />

    @RenderSection("StyleSection", required: false)
</head>
<!-- END: Head-->
<!-- BEGIN: Body-->

<body class="">
    <!-- begin app -->
    <div class="app">
        <!-- begin app-wrap -->
        <div class="app-wrap">
            <!--start login contant-->
            <div class="app-content content">
                <div class="content-wrapper">
                    <div class="content-wrapper-before"></div>
                    <div class="content-header row">
                    </div>
                    <div class="content-body">
                        <section class="flexbox-container">
                            <div class="col-12 d-flex align-items-center justify-content-center">
                                <div class="col-lg-4 col-md-6 col-10 box-shadow-2 p-0">
                                    <div class="card border-grey border-lighten-3 px-1 py-1 m-0">
                                        <div class="card-header border-0">
                                            <div class="text-center mb-1">
                                                <img src="~/ChameleonAdmin/app-assets/images/logo/logo-80x80.png" alt="branding logo">
                                            </div>
                                            <div class="font-large-1  text-center">
                                                Member Login
                                            </div>
                                        </div>
                                        <div class="card-content">

                                            <div class="card-body">
                                                @RenderBody()
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>

                    </div>
                </div>
            </div>
            <!--end login contant-->
        </div>
        <!-- end app-wrap -->
    </div>
    <!-- end app -->
    <!-- plugins -->

    <script src="~/ChameleonAdmin/app-assets/vendors/js/vendors.min.js" type="text/javascript"></script>  
    <script src="~/ChameleonAdmin/app-assets/vendors/js/extensions/toastr.min.js" type="text/javascript"></script>   
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>

